  "markup": {
    "language": "html",
    "content": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n  <head>\r\n    <meta charset=\"UTF-8\" />\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\" />\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\r\n    <title>Form</title>\r\n    <link rel=\"stylesheet\" href=\"css/styles.css\" />\r\n  </head>\r\n  <body>\r\n    <!--Headers-->\r\n    <div id=\"headers\">\r\n      <header id=\"main-header\">\r\n        <h1>the collection of my projects</h1>\r\n        <a class=\"project\" href=\"https://karimhatemhosny.github.io/karim-website/\" target=\"_blank\"\r\n          >HTML project</a\r\n        >\r\n      </header>\r\n\r\n      <header id=\"second-header\">\r\n        <h1>Sections</h1>\r\n        <ul>\r\n          <li class=\"middle\"><a class=\"list-links\" href=\"#form-id\">Form</a></li>\r\n          <li class=\"middle\">\r\n            <a class=\"list-links\" href=\"#wor\">Would You Rather</a>\r\n          </li>\r\n        </ul>\r\n      </header>\r\n    </div>\r\n\r\n    <h1 id=\"survey-title\">My Survey Form</h1>\r\n\r\n    <!--Form-->\r\n    <form action=\"\" id=\"form-id\">\r\n      <div class=\"form\">\r\n        <label>Name</label>\r\n        <input type=\"text\" name=\"name\" placeholder=\"Enter Name\" />\r\n      </div>\r\n      <br />\r\n\r\n      <div class=\"form\">\r\n        <label>Email</label>\r\n        <input type=\"email\" name=\"email\" placeholder=\" Enter Email\" />\r\n      </div>\r\n      <br />\r\n\r\n      <div class=\"form\">\r\n        <label>Password</label>\r\n        <input type=\"password\" name=\"pass\" placeholder=\"Enter Password\" />\r\n      </div>\r\n\r\n      <div class=\"form\">\r\n        <label>Age</label>\r\n        <input type=\"number\" name=\"age\" placeholder=\" Enter Your Age\" />\r\n      </div>\r\n      <br />\r\n\r\n      <div class=\"form\">\r\n        <label>Phone number</label>\r\n        <input\r\n          type=\"number\"\r\n          name=\"phone number\"\r\n          placeholder=\"Enter Phone Number\"\r\n        />\r\n      </div>\r\n      <br />\r\n\r\n      <div class=\"form\">\r\n        <label>Date of birth</label>\r\n        <input type=\"date\" name=\"dob\" />\r\n      </div>\r\n      <br />\r\n\r\n      <div class=\"form\">\r\n        <label>Favorite color</label>\r\n        <select name=\"favorite color\">\r\n          <option value=\"red\">Red</option>\r\n          <option value=\"orange\">Orange</option>\r\n          <option value=\"yellow\">Yellow</option>\r\n          <option value=\"green\">Green</option>\r\n          <option value=\"cyan\">Cyan</option>\r\n          <option value=\"blue\">Blue</option>\r\n          <option value=\"purple\">Purple</option>\r\n        </select>\r\n      </div>\r\n      <br />\r\n\r\n      <div class=\"form\">\r\n        <label>Gender</label>\r\n        <select name=\"gender\">\r\n          <option value=\"male\">Male</option>\r\n          <option value=\"female\">Female</option>\r\n        </select>\r\n      </div>\r\n      <br />\r\n      <div class=\"form\">\r\n        <label>Favorite Fruit</label><br />\r\n        <select name=\"fruit\">\r\n          <option value=\"apple\">Apple</option>\r\n          <option value=\"orange\">Orange</option>\r\n          <option value=\"banana\">Banana</option>\r\n          <option value=\"mango\">Mango</option>\r\n          <option value=\"watermelon\">Watermelon</option>\r\n        </select>\r\n      </div>\r\n\r\n      <div class=\"form\">\r\n        <label>What word better discribes your personality?</label><br />\r\n        <select name=\"personality\">\r\n          <option value=\"friendly\">Friendly</option>\r\n          <option value=\"helpful\">Helpful</option>\r\n          <option value=\"sportive\">Sportive</option>\r\n          <option value=\"agressive\">Agressive</option>\r\n          <option value=\"quite\">Quite</option>\r\n          <option value=\"other\">Other</option>\r\n        </select>\r\n      </div>\r\n\r\n      <div class=\"form\">\r\n        <label>Wake up time</label>\r\n        <input type=\"time\" />\r\n      </div>\r\n\r\n      <div class=\"form\">\r\n        <label>Sleep time</label>\r\n        <input type=\"time\" />\r\n      </div>\r\n\r\n      <div class=\"radio\">\r\n        <label>What would you do in your free time?</label> <br />\r\n        <input type=\"radio\" name=\"free-time\" />\r\n        <label>Play video games</label>\r\n        <br />\r\n\r\n        <input type=\"radio\" name=\"free-time\" />\r\n        <label>Learn something new</label>\r\n        <br />\r\n\r\n        <input type=\"radio\" name=\"free-time\" />\r\n        <label>Sleep</label>\r\n        <br />\r\n\r\n        <input type=\"radio\" name=\"free-time\" />\r\n        <label>Other</label>\r\n        <br />\r\n      </div>\r\n      <br />\r\n      <br />\r\n\r\n      <label>Check all the languages that you can code</label>\r\n      <br />\r\n      <input type=\"checkbox\" name=\"Javascript\" />\r\n      <label>Javascript</label>\r\n      <br />\r\n      <input type=\"checkbox\" name=\"Typescript\" />\r\n      <label>Typescript</label>\r\n      <br />\r\n      <input type=\"checkbox\" name=\"HTML\" />\r\n      <label>HTML</label>\r\n      <br />\r\n      <input type=\"checkbox\" name=\"CSS\" />\r\n      <label>CSS</label>\r\n      <br />\r\n      <input type=\"checkbox\" name=\"C#\" />\r\n      <label>C#</label>\r\n      <br />\r\n      <input type=\"checkbox\" name=\"C\" />\r\n      <label>C</label>\r\n      <br />\r\n      <input type=\"checkbox\" name=\"C++\" />\r\n      <label>C++</label>\r\n      <br />\r\n      <input type=\"checkbox\" name=\"Python\" />\r\n      <label>Python</label>\r\n      <br />\r\n      <input type=\"checkbox\" name=\"React\" />\r\n      <label>React</label>\r\n      <br />\r\n      <br />\r\n\r\n      <!--Textarea-->\r\n      <label>Message</label><br />\r\n      <textarea placeholder=\"Any thing to add\"></textarea>\r\n      <br />\r\n\r\n      <p class=\"middle\" id=\"test\">make sure to submit of course</p>\r\n    </form>\r\n    <!--/form-->\r\n    <hr />\r\n\r\n    <div id=\"wor\">\r\n      <div class=\"radio\">\r\n        <h1>Would You Rather</h1>\r\n        <br />\r\n        <label>Eat</label>\r\n        <br />\r\n\r\n        <input type=\"radio\" name=\"eat\" />\r\n        <label>Raw Fish</label>\r\n        <br />\r\n\r\n        <input type=\"radio\" name=\"eat\" />\r\n        <label>Rotten Chocolate</label>\r\n        <br />\r\n\r\n        <input type=\"radio\" name=\"eat\" />\r\n        <label>3 Ice Creams in Winter</label>\r\n        <br />\r\n        <br />\r\n\r\n        <label>Do</label>\r\n        <br />\r\n\r\n        <input type=\"radio\" name=\"do\" />\r\n        <label>Swim for 3 hours non-stop</label>\r\n        <br />\r\n\r\n        <input type=\"radio\" name=\"do\" />\r\n        <label>Wear 5 T-shirts in Summer for a week</label>\r\n        <br />\r\n\r\n        <input type=\"radio\" name=\"do\" />\r\n        <label>Look at the sun for 1 minute</label>\r\n        <br />\r\n        <br />\r\n\r\n        <label>Excrise</label>\r\n        <br />\r\n\r\n        <input type=\"radio\" name=\"excrise\" />\r\n        <label>Do 50 push ups in a row</label>\r\n        <br />\r\n\r\n        <input type=\"radio\" name=\"excrise\" />\r\n        <label>Run for 1 hour as fast as you can</label>\r\n        <br />\r\n\r\n        <input type=\"radio\" name=\"excrise\" />\r\n        <label>Hop 100 times non-stop</label>\r\n        <br />\r\n        <br />\r\n\r\n        <!--submit-->\r\n        <p class=\"middle\"><button onclick=\"myFunction()\">Submit</button></p>\r\n        <br />\r\n\r\n        <p class=\"middle\">If you want to know more here is the needed info</p>\r\n        <p></p>\r\n        <table border=\"1\">\r\n          <thead>\r\n            <tr>\r\n              <th>Name</th>\r\n              <th>Age</th>\r\n              <th>Gender</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr>\r\n              <td>Karim</td>\r\n              <td>10</td>\r\n              <td>Male</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <br />\r\n    </div>\r\n\r\n    <!--Last thing before footer-->\r\n    <p class=\"middle\"><a class=\"project\" href=\"#\">Back to top</a></p>\r\n\r\n    <ul>\r\n      <li class=\"middle\"><a class=\"list-links\" href=\"#form-id\">Form</a></li>\r\n      <li class=\"middle\">\r\n        <a class=\"list-links\" href=\"#wor\">Would You Rather</a>\r\n      </li>\r\n    </ul>\r\n\r\n    <!--Footer-->\r\n    <footer id=\"main-footer\">\r\n      <ul>\r\n        <li><div id=\"cursor\" onclick=\"openModal()\">Share</div></li>\r\n        <li><div id=\"cursor\" onclick=\"openModalForMore()\">More...</div></li>\r\n      </ul>\r\n    </footer>\r\n\r\n    <!-- Modal -->\r\n    <div class=\"modal\">\r\n      <div class=\"screen\"></div>\r\n      <div class=\"modal-content\">\r\n        <h1>Share via...</h1>\r\n        <br />\r\n\r\n        <div class=\"flex\">\r\n          <a\r\n            class=\"flex-item\"\r\n            href=\"https://mail.google.com/mail/u/0/#inbox?compose=CllgCKCJDfJLfnMwPZkCtKSptdwHvzFwddqHLKrSZzxzdRJpZFcXvQsdDLpFDkgtbHRKjKGHXJV\"\r\n            target=\"_blank\"\r\n            >Email</a\r\n          >\r\n\r\n          <a class=\"flex-item\" href=\"https://www.yahoo.com/\" target=\"_blank\"\r\n            >Yahoo</a\r\n          >\r\n\r\n          <a class=\"flex-item\" href=\"https://www.twitter.com/\" target=\"_blank\"\r\n            >Twitter</a\r\n          >\r\n\r\n          <a class=\"flex-item\" href=\"\" target=\"_blank\">hi</a>\r\n        </div>\r\n\r\n        <!--close button-->\r\n        \r\n        <div class=\"close\">\r\n          <button onclick=\"closeModal()\">Back</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!--another modal-->\r\n\r\n    <div class=\"more\">\r\n      <div class=\"screen\"></div>\r\n      <div class=\"modal-content\">\r\n        <h1>More...</h1>\r\n        <br />\r\n\r\n        <div class=\"flex\">\r\n          <a\r\n            class=\"flex-item\"\r\n            href=\"https://mail.google.com/mail/u/0/#inbox?compose=CllgCKCJDfJLfnMwPZkCtKSptdwHvzFwddqHLKrSZzxzdRJpZFcXvQsdDLpFDkgtbHRKjKGHXJV\"\r\n            target=\"_blank\"\r\n            >Contact</a\r\n          >\r\n\r\n          <a class=\"flex-item\" href=\"../New website.zip/\" download target=\"_blank\"\r\n            >Download the website</a\r\n          >\r\n        </div>\r\n        <div class=\"close\">\r\n          <button onclick=\"closeModalForMore()\">Back</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <script src=\"script.js\"></script>\r\n  </body>\r\n</html>\r\n",
